<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>
	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>INIntentsSupported</key>
	<array>
		<string>Intent</string>
		<string>Intent1</string>
	</array>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSHealthClinicalHealthRecordsShareUsageDescription</key>
	<string>We read youe Health Records</string>
	<key>NSHealthShareUsageDescription</key>
	<string>We read your workout data to show live dive and heart rate stats.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>We write workout updates to log your dives and heart rate data.</string>
	<key>NSMotionUsageDescription</key>
	<string>We use motion sensors to detect submersion depth during your dives.</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
	</array>
	<key>WKApplication</key>
	<true/>
	<key>WKBackgroundModes</key>
	<array>
		<string>underwater-depth</string>
		<string>workout-processing</string>
	</array>
	<key>WKWatchOnly</key>
	<true/>
</dict>
</plist>
